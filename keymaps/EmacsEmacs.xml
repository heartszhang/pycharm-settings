<keymap version="1" name="EmacsEmacs" parent="Emacs">
  <action id="ActivatePythonConsoleToolWindow">
    <keyboard-shortcut first-keystroke="alt 0" />
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl alt minus" />
  </action>
  <action id="CollapseRegionRecursively">
    <keyboard-shortcut first-keystroke="ctrl alt subtract" />
  </action>
  <action id="ExecuteInPyConsoleAction">
    <keyboard-shortcut first-keystroke="shift alt e" />
    <keyboard-shortcut first-keystroke="ctrl c" />
  </action>
  <action id="ExpandRegionRecursively">
    <keyboard-shortcut first-keystroke="ctrl alt add" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl alt equals" />
  </action>
  <action id="QuickChangeScheme" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt back_slash" />
  </action>
  <action id="RunJsbtTask" />
  <action id="ShowUsages">
    <keyboard-shortcut first-keystroke="alt f11" />
  </action>
</keymap>